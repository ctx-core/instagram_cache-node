{
	"name": "@ctx-core/instagram_cache-node",
	"version": "0.0.22",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache-node#readme",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.48.0",
		"@aws-sdk/client-s3": "^3.48.0",
		"@ctx-core/cli-args": "workspace:^11.0.24",
		"@ctx-core/instagram": "workspace:^6.0.112",
		"@ctx-core/instagram_cache": "workspace:^11.0.18",
		"@ctx-core/sleep": "workspace:^8.0.36",
		"jsdom": "^19.0.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.133",
		"@types/jsdom": "^16.2.14",
		"@types/selenium-webdriver": "^4.0.16",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"svelte": "./lib/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache-node.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache-node/issues"
	},
	"sideEffects": false
}