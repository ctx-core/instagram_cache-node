{
	"name": "@ctx-core/instagram_cache-node",
	"version": "0.2.83",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache-node#readme",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./created_time_",
		"./dynamodb",
		"./medium_href_a_",
		"./s3__medium_pathname_a__put",
		"./webdriver__scrape__cache__put"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.465.0",
		"@aws-sdk/client-s3": "^3.465.0",
		"@ctx-core/cli-args": "^11.4.37",
		"@ctx-core/env": "^17.4.41",
		"@ctx-core/instagram": "^6.3.70",
		"@ctx-core/instagram_cache": "^15.1.79",
		"@ctx-core/sleep": "^8.2.37",
		"jsdom": "^23.0.1",
		"selenium-webdriver": "^4.15.0"
	},
	"devDependencies": {
		"@aws-sdk/types": "^3.465.0",
		"@types/jsdom": "^21.1.6",
		"@types/selenium-webdriver": "^4.1.20",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache-node.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache-node/issues"
	},
	"sideEffects": false
}